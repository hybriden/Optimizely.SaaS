{
  "name": "proxima-saas",
  "version": "0.1.0",
  "private": true,
  "license": "Apache-2.0",
  "packageManager": "yarn@4.9.2",
  "scripts": {
    "dev": "yarn generate:all && next dev",
    "build": "yarn generate:all && next build",
    "build:full": "yarn compile && yarn generate:all && next build",
    "start": "next start",
    "lint": "next lint",
    "compile": "graphql-codegen",
    "watch": "graphql-codegen -w",
    "generate": "node scripts/generate-component.mjs",
    "generate:all": "yarn generate:factories && yarn generate:registries && yarn generate:fragments && yarn generate:functions",
    "generate:factories": "node scripts/generate-factories.mjs",
    "generate:registries": "node scripts/generate-registry.mjs && node scripts/generate-client-registry.mjs",
    "generate:fragments": "node scripts/generate-shared-fragments.mjs",
    "generate:functions": "node scripts/generate-functions.mjs",
    "cleanup": "node scripts/cleanup-stale-types.mjs",
    "opti:login": "npx @optimizely/cms-cli@latest login",
    "opti:push": "npx @optimizely/cms-cli@latest config push optimizely.config.mjs --force",
    "opti:push:safe": "npx @optimizely/cms-cli@latest config push optimizely.config.mjs"
  },
  "dependencies": {
    "@optimizely/cms-sdk": "^0.1.0-alpha.11",
    "dotenv": "^17.1.0",
    "dotenv-expand": "^12.0.3",
    "graphql": "^16.11.0",
    "graphql-request": "^6.1.0",
    "next": "^16.0.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "server-only": "^0.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/client-preset": "^5.1.1",
    "@graphql-codegen/fragment-matcher": "^6.0.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@optimizely/cms-cli": "^0.1.0-alpha.9",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^22.16.2",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.3",
    "@yarnpkg/sdks": "^3.2.2",
    "chalk": "^5.4.1",
    "eslint": "^9.38.0",
    "figures": "^6.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3"
  }
}
