{
  "name": "optimizely-saas-starter",
  "version": "0.1.0",
  "private": true,
  "license": "Apache-2.0",
  "packageManager": "yarn@4.9.2",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:full": "graphql-codegen && next build",
    "start": "next start",
    "lint": "next lint",
    "compile": "graphql-codegen",
    "watch": "graphql-codegen -w",
    "generate": "node scripts/generate-component.mjs",
    "cleanup": "node scripts/cleanup-stale-types.mjs",
    "opti:login": "npx @optimizely/cms-cli@latest login",
    "opti:push": "npx @optimizely/cms-cli@latest config push optimizely.config.mjs --force",
    "opti:push:safe": "npx @optimizely/cms-cli@latest config push optimizely.config.mjs"
  },
  "dependencies": {
    "@optimizely/cms-sdk": "^0.1.0-alpha.11",
    "dotenv": "^17.1.0",
    "dotenv-expand": "^12.0.3",
    "graphql": "^16.11.0",
    "graphql-request": "^6.1.0",
    "next": "^16.0.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "server-only": "^0.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/client-preset": "^5.1.1",
    "@graphql-codegen/fragment-matcher": "^6.0.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@optimizely/cms-cli": "^0.1.0-alpha.9",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^22.16.2",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.3",
    "@yarnpkg/sdks": "^3.2.2",
    "chalk": "^5.4.1",
    "eslint": "^9.38.0",
    "figures": "^6.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3"
  },
  "resolutions": {
    "@graphql-codegen/visitor-plugin-common": "file:./packages/graphql-codegen-visitor-plugin-common-v5.8.0-patched.tgz"
  },
  "comments": {
    "resolutions": "Patched version fixes TypeScript issues with fragment generation. Remove when official @graphql-codegen/visitor-plugin-common > v5.8.0 is available."
  }
}
